{
  "name": "@architect/architect",
  "homepage": "https://arc.codes",
  "version": "11.3.0",
  "description": "Create, deploy, and maintain next-generation AWS cloud function-based serverless infrastructure with full local, offline workflows, and more.",
  "main": "src/index.js",
  "bin": {
    "arc": "src/index.js"
  },
  "scripts": {
    "test": "npm run lint && npm run test:integration && npm run coverage",
    "test:unit": "cross-env tape 'test/unit/**/*-test.js'",
    "test:integration": "cross-env tape 'test/integration/**/*-test.js'",
    "lint": "eslint . --fix",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "rc": "npm version prerelease --preid RC"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/architect/architect.git"
  },
  "bugs": {
    "url": "https://github.com/architect/architect/issues"
  },
  "author": "Brian LeRoux <b@brian.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@architect/create": "6.0.0",
    "@architect/deploy": "5.1.1-RC.0",
    "@architect/destroy": "5.0.0",
    "@architect/env": "5.0.0",
    "@architect/hydrate": "5.0.0",
    "@architect/inventory": "5.0.0",
    "@architect/logs": "6.0.0",
    "@architect/sandbox": "8.0.0-RC.1",
    "@architect/utils": "5.0.0",
    "@aws-lite/client": "0.23.2",
    "update-notifier-cjs": "5.1.6"
  },
  "devDependencies": {
    "@architect/eslint-config": "~3.0.0",
    "cross-env": "10.0.0",
    "eslint": "9.36.0",
    "nyc": "17.1.0",
    "proxyquire": "~2.1.3",
    "tape": "5.9.0"
  },
  "keywords": [
    "amazon",
    "api gateway",
    "arc",
    "architect",
    "aws",
    "cloud functions",
    "cloud",
    "dynamodb",
    "framework",
    "lambda",
    "s3",
    "serverless",
    "sns"
  ]
}
